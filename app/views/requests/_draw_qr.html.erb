<div>
  <table id="bitcoin-qr">
    <% qr.modules.each_index do |x| -%>
      <tr>
        <% qr.modules.each_index do |y| -%>
          <% if qr.dark?(x,y) -%>
            <td class="black">
            </td>
          <% else -%>
            <td class="white">
            </td>
          <% end -%>
        <% end -%>
      </tr>
    <% end -%>
  </table>
</div>
